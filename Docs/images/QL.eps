%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 75 310 495 530
%%Title: QL.eps
%%Creator: Luigi Ballabio
%%CreationDate: Thu Aug 15 15:35:02 2002
%%EndComments

% A4 paper (210x297 mm)
72 2.54 div dup scale   % use centimeters
10.5 14.85 translate    % origin at center of page


% strokes
/thick { 0.05 setlinewidth } def
/thin  { 0.01 setlinewidth } def

/H 6.0 def         % character height
/h H 2.0 div def   % half height
/w 0.9 def         % serif width
/m 0.5 def         % "margin" of thin strokes 

% Q
gsave
-4.0 0.0 translate
thick
newpath 0.0 0.0 h 0 360 arc stroke
gsave
h w sub h div 1.0 scale
newpath 0.0 0.0 h 0 360 arc stroke
grestore
thin
newpath 0.0 h m add moveto 0.0 h m add neg lineto stroke
newpath h m add 0.0 moveto h m add neg 0.0 lineto stroke
newpath -2.0 h moveto 2.0 h lineto stroke
newpath -2.0 h neg moveto 2.0 h neg lineto stroke
newpath h -2.0 moveto h 2.0 lineto stroke
newpath h neg -2.0 moveto h neg 2.0 lineto stroke
newpath 0.0 0.0 moveto h 2.0 sqrt div dup neg lineto 
currentpoint stroke
thick
/r 2.0 sqrt dup 1.0 add div 0.6 mul def
/R 2.0 sqrt dup 1.0 sub div 0.27 mul def
newpath moveto 
2.4 -2.4 lineto 
gsave
r 2.0 sqrt div neg dup rmoveto 
currentpoint currentpoint
grestore
r 45 -90 arcn 
r R add 2.0 sqrt div 0.33 add 0.0 rlineto
gsave
0.0 R rmoveto 
currentpoint currentpoint
grestore
R -90 -135 arcn 
closepath stroke
thin
newpath R 0 360 arc stroke
newpath r 0 360 arc stroke
grestore

% L
gsave
1.0 -3.0 translate
/r 0.65 def
/l 3.5 def
newpath
thick
0.0 0.0 moveto
gsave
0.0 r rmoveto currentpoint currentpoint
grestore
r -90 0 arc
0.0 H r 2.0 mul sub rlineto 
gsave 
r neg 0.0 rmoveto currentpoint currentpoint
grestore
r 0 90 arc
w r 2.0 mul add 0.0 rlineto
gsave 
0.0 r neg rmoveto currentpoint currentpoint
grestore
r 90 180 arc
0.0 H r 2.0 mul sub neg rlineto 
gsave 
r 0.0 rmoveto currentpoint currentpoint
grestore
r 180 270 arc
l r 2.0 mul sub 0.0 rlineto
gsave 
0.0 r rmoveto currentpoint currentpoint
grestore
r -90 0 arc
0.0 r neg rlineto
closepath
stroke
thin
5 { newpath r 0 360 arc stroke } repeat
newpath m neg 0.0 moveto m 2.0 mul r w l add add add 0.0 rlineto stroke
newpath m neg H moveto m 2.0 mul r 2.0 mul w add add 0.0 rlineto stroke
newpath r m neg moveto 0.0 m 2.0 mul H add rlineto stroke
newpath r w add m neg moveto 0.0 m 2.0 mul H add rlineto stroke
newpath r w l add add m neg moveto 0.0 m 2.0 mul r add rlineto stroke
grestore

showpage
%%EOF